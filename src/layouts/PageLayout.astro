---
import React from "react"
import BaseLayout from "$src/layouts/BaseLayout.astro"

import SkipLinks from "$src/components/skip-links/skip-links.jsx"
import Header from "$src/components/header/header.astro"
import SocialLinks from "$src/components/social-links.jsx"
import Footer from "$src/components/footer.jsx"
const { title } = Astro.props
---

<BaseLayout {title}>
  <div class="wrapper">
    <SkipLinks />
    <Header />
    <main><slot /></main>
    <!-- <SocialLinks /> -->
    <Footer />
  </div>

  <script src="$src/js/home.js"></script>
  <script src="$src/js/animations.js"></script>
</BaseLayout>
